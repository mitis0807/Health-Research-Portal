  "scripts": {
    "webpack": "webpack",
    "dev": "npm run webpack -- --mode development --watch",
    "build": "npm run webpack -- --mode production",
    "start": "PORT=3006 npm-run-all --parallel open:client lift",
    "start:debug": "npm-run-all --parallel open:client debug",
    "startDocker": "npm-run-all --parallel open:client dockerLift ",
    "open:client": "webpack-dev-server --mode development --open --host 0.0.0.0",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:stage": "webpack --mode staging",
    "clean": "rimraf .tmp && mkdirp .tmp/public",
    "lift": "PORT=1340 NODE_ENV=development sails lift",
    "dockerLift": "PORT=1340 NODE_ENV=docker sails lift",
    "debug": "node --inspect app.js",
    "test": "npm run lint && npm run custom-tests && echo 'Done.'",
    "lint": "eslint ./api",
    "custom-tests": "echo \"(No other custom tests yet.)\" && echo",
    "analyze": "source-map-explorer /.tmp/public/.*",
    "lintReport": "eslint . -f json -o eslint.json",
